<?xml version="1.0" encoding="UTF-8"?>
<catalogo xmlns:os="http://www.example.com/os"
          xmlns:office="http://www.example.com/office">
  
  <!-- Esto es una Sección de Software de Sistemas Operativos -->
  <os:sistemas_operativos>
