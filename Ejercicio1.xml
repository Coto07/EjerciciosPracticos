    <!-- Segundo software de sistema operativo -->
    <os:software>
      <os:nombre>Ubuntu 22.04</os:nombre>
      <os:version>22.04 LTS</os:version>
      <os:desarrollador>Canonical</os:desarrollador>
      <os:url>https://ubuntu.com/</os:url>
    </os:software>

    <!-- Tercer software de sistema operativo -->
    <os:software>
      <os:nombre>macOS Ventura</os:nombre>
      <os:version>13.0</os:version>
      <os:desarrollador>Apple</os:desarrollador>
      <os:url>https://www.apple.com/macos/ventura/</os:url>
    </os:software>
  </os:sistemas_operativos>

