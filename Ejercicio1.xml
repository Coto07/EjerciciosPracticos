    <os:software>
      <os:nombre>Windows 11</os:nombre> <!-- Nombre del software -->
      <os:version>21H2</os:version> <!-- Versión del software -->
      <os:desarrollador>Microsoft</os:desarrollador> <!-- Desarrollador del software -->
      <os:url>https://www.microsoft.com/windows</os:url> <!-- URL para más información -->
